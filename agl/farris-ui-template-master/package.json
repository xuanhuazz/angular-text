    "minify-monaco": "cleancss --level 1  --output src/assets/monaco/editor.main.css tmp/editor.main.css",
    "css-all": "gulp all",
    "css-update": "gulp update",
    "css-docs": "gulp docs",
    "css-publish": "gulp publish",
